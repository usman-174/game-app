generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Game {
  id                 Int    @id @default(autoincrement())
  percentRecommended Float
  numReviews         Int
  topCriticScore     Float
  tier               String
  name               String
  url                String
  firstReleaseDate   Int
}
